{
  "name": "Environmental Scraper Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.amazon.ca/*"],
      "js": ["ecoscore.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "chrome_url_overrides": {
    "newtab": "tab.html"
  }
}